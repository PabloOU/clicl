;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; trueserver.test;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; reset;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(resetsystem)*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; revise, errors, revisions, notifications, reactions;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(revisions '(p a) *mytrueserver*)((P A))(revisions '(q a) *mytrueserver*)((Q A))(revisions '(and (not (p b)) (q b)) *mytrueserver*)((AND (NOT (P B)) (Q B)))(revisions '(<= (r ?x) (p ?x) (q ?x)) *mytrueserver*)((<= (R ?X) (P ?X) (Q ?X)));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; askp, askx, asks;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(define-theory *mytrueserver* ""  '((p a)    (q a)    (and (not (p b)) (q b))    (<= (r ?x) (p ?x) (q ?x))))*(askp '(p a) nil *mytrueserver*)T(askp '(and (not (p b)) (q b)) nil *mytrueserver*)T(askp '(and (q b) (not (p b))) nil *mytrueserver*)NIL(askp '(<= (r ?x) (p ?x) (q ?x)) nil *mytrueserver*)T(askp '(r a) nil *mytrueserver*)NIL;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; indexps, contents;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(indexps 'a *mytrueserver*)((P A) (Q A))(indexps 'p *mytrueserver*)((P A) (AND (NOT (P B)) (Q B)) (<= (R ?X) (P ?X) (Q ?X)))(indexps 'q *mytrueserver*)((Q A) (AND (NOT (P B)) (Q B)) (<= (R ?X) (P ?X) (Q ?X)));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; contents;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(contents *mytrueserver*)((P A) (Q A) (AND (NOT (P B)) (Q B)) (<= (R ?X) (P ?X) (Q ?X)))(resetsystem)*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;