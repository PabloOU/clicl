(FORMNAME PLFEATURE) 
(CONSTRAINTS '((OR (NOT E) (NOT M)) (OR E M) (<=> O (OR (FS "Constant folding") (FS "Short circuit"))) (=> (FS "Short circuit") C))) 
(DEFINITIONS 'NIL) 
(OPTION ALLOWCONFLICTS T) 
(WIDGET :NAME E :REQ NIL :INIT (LISTOF "true") :DESC "Evaluation" :STYLE CHECKBOX :INCUNDEF NIL :UNIQUE T :TYPENAME BOOLEAN) 
(WIDGET :NAME M :REQ NIL :INIT NIL :DESC "Machine" :STYLE CHECKBOX :INCUNDEF NIL :UNIQUE T :TYPENAME BOOLEAN) 
(WIDGET :NAME O :REQ NIL :INIT (LISTOF "true") :DESC "Optimizer" :STYLE CHECKBOX :INCUNDEF NIL :UNIQUE T :TYPENAME BOOLEAN) 
(WIDGET :NAME FS :REQ NIL :INIT (LISTOF "Constant folding") :DESC "Optimization" :STYLE RADIO :INCUNDEF NIL :UNIQUE T :TYPENAME FS_TYPE) 
(WIDGET :NAME C :REQ NIL :INIT NIL :DESC "Conditional" :STYLE CHECKBOX :INCUNDEF NIL :UNIQUE T :TYPENAME BOOLEAN) 
(TYPE FS_TYPE (LISTOF "None" "Constant folding" "Short circuit")) 