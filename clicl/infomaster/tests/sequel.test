;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Sequel Tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(restartsystem)*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; sql tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(sql '(ask-all (?x ?y) (domain ?x ?y)))"SELECT DISTINCT domain.1,domain.2  FROM domain"(sql '(ask-all (?x ?y) (and (domain ?u ?v) (range ?x ?y) (same ?u ?x) (same ?v ?y))))"SELECT DISTINCT range.1,range.2  FROM domain,range  WHERE domain.1=range.1 AND domain.2=range.2"(sql '(ask-all (?x) (or (domain ?x class) (range ?x class))))"SELECT DISTINCT domain.1  FROM domain  WHERE domain.2=class UNION SELECT DISTINCT range.1  FROM range  WHERE range.2=class"(sql '(ask-all (?x) (and (arity ?x ?y) (> ?y 2))))"SELECT DISTINCT arity.1  FROM arity  WHERE arity.2>2";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; acl tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(samep (acl "SELECT DISTINCT domain.1,domain.2 FROM domain")       '(ask-all (?x ?y) (domain ?x ?y)))T(samep (acl "SELECT DISTINCT range.1,range.2 FROM domain,range WHERE domain.1=range.1 AND domain.2=range.2")       '(ASK-ALL (?33 ?34) (AND (DOMAIN ?33 ?34) (RANGE ?33 ?34))))T(samep (acl "SELECT DISTINCT domain.1 FROM domain WHERE domain.2=class             UNION              SELECT DISTINCT range.1 FROM range WHERE range.2=class")       '(UNION (ASK-ALL ?203 (DOMAIN ?203 CLASS)) (ASK-ALL ?205 (RANGE ?205 CLASS))))T(samep (acl "SELECT DISTINCT arity.1 FROM arity WHERE arity.2>2")       '(ASK-ALL ?207 (AND (ARITY ?207 ?208) (> ?208 2))))T;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;