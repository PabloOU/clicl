;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Album tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Loader;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(resetsystem)*(load "gullible:infomaster:examples:album:load.mcl")*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Data Tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(checkclass 'photo standard)0(checkclass 'site standard)0(checkclass 'state standard)0(checkclass 'country standard)0(checkclass 'continent standard)0;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Baselevel tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(> (length (request '(ask-all ?x (photo.instance ?x)) nil *gui*)) 0)T(> (length (request '(ask-all ?x (site.instance ?x)) nil *gui*)) 0)T(> (length (request '(ask-all ?x (state.instance ?x)) nil *gui*)) 0)T(length (request '(ask-all ?x (country.instance ?x)) nil *gui*))184(length (request '(ask-all ?x (continent.instance ?x)) nil *gui*))7;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Agent tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(length (asks '?x '(isa ?x factserver) nil *manager*))1(length (asks '?x '(isa ?x viewserver) nil *manager*))2(length (asks '?x '(isa ?x fullserver) nil *manager*))1(length (asks '?x '(isa ?x dataserver) nil *manager*))2(length (asks '?x '(isa ?x diffserver) nil *manager*))1(length (asks '?x '(isa ?x ruleserver) nil *manager*))2(length (asks '?x '(isa ?x fastserver) nil *manager*))2(length (asks '?x '(isa ?x translator) nil *manager*))1(length (asks '?x '(isa ?x transformer) nil *manager*))2(length (asks '?x '(isa ?x integrator) nil *manager*))1(length (asks '?x '(isa ?x facilitator) nil *manager*))2(length (asks '?x '(isa ?x interface) nil *manager*))1(length (asks '?x '(isa ?x agent) nil *manager*))18;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Class tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(request '(ask-if (isa photo class)) nil *manager*)T(request '(ask-if (isa site class)) nil *manager*)T(request '(ask-if (isa state class)) nil *manager*)T(request '(ask-if (isa country class)) nil *manager*)T(request '(ask-if (isa continent class)) nil *manager*)T;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Relation tests;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(request '(ask-one ?y (domain photo.subject ?y)) nil *manager*)photo(request '(ask-one ?y (range photo.subject ?y)) nil *manager*)person(request '(ask-one ?y (unique photo.subject ?y)) nil *manager*)no(request '(ask-one ?y (total photo.subject ?y)) nil *manager*)no(request '(ask-one ?y (searchstyle photo.subject ?y)) nil *manager*)dropdownlist(resetsystem)*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;