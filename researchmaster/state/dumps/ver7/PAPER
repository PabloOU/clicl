    (paper.instance    petrie2004)
    (paper.bibtex      petrie2004 article)
    (paper.author      petrie2004 charlesjpetrie)
    (paper.author      petrie2004 michaelrgenesereth)
    (paper.author      petrie2004 hansbjornsson)
