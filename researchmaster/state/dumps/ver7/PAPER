    (paper.instance    kautz92)
    (paper.bibtex      kautz92 inproceedings)
    (paper.author      kautz92 henrykautz)
    (paper.author      kautz92 bartselman)
    (paper.title       kautz92 "Forming Concepts for Fast Inference")
    (paper.publication kautz92 ecai-kr)
    (paper.startpage   kautz92 200)
    (paper.endpage     kautz92 215)
    (paper.url         kautz92 "http://citeseer.ist.psu.edu/kautz92forming.html")
    (paper.year        kautz92 1992)
