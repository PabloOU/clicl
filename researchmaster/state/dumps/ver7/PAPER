    (paper.publication hinrichs2009practical wren)
    (paper.startpage   hinrichs2009practical "1")
    (paper.endpage     hinrichs2009practical "10")
    (paper.url         hinrichs2009practical "papers/hinrichs2009practical.pdf")
    (paper.year        hinrichs2009practical 2009)
    (paper.topic       hinrichs2009practical security)
