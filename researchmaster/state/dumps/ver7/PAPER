    (paper.instance    hinrichs2005axiom)
    (paper.bibtex      hinrichs2005axiom inproceedings)
    (paper.author      hinrichs2005axiom timothylhinrichs)
    (paper.author      hinrichs2005axiom michaelrgenesereth)
    (paper.title       hinrichs2005axiom "Axiom Schemata as Metalevel Axioms: Model Theory")
    (paper.publication hinrichs2005axiom aaai)
    (paper.startpage   hinrichs2005axiom "294")
    (paper.endpage     hinrichs2005axiom "299")
